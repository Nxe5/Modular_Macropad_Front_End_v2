{
  "id": "open_iterm_spotlight",
  "name": "Open iTerm Spotlight",
  "description": "Quickly opens iTerm using Spotlight Search. This macro automates the process of opening the terminal, saving time for developers and power users who frequently need terminal access. Uses Command + Space to open Spotlight and types 'iterm' to launch iTerm2.",
  "commands": [
    {
      "type": "key_press",
      "report": ["0x08", "0x00", "0x00", "0x00", "0x00", "0x00", "0x00", "0x00"]
    },
    {
      "type": "delay",
      "milliseconds": 50
    },
    {
      "type": "type_text",
      "text": "iterm"
    },
    {
      "type": "delay",
      "milliseconds": 50
    },
    {
      "type": "key_up",
      "report": ["0x00", "0x00", "0x00", "0x00", "0x00", "0x00", "0x00", "0x00"]
    },
    {
      "type": "delay",
      "milliseconds": 100
    },
    {
      "type": "key_press",
      "report": ["0x00", "0x00", "0x28", "0x00", "0x00", "0x00", "0x00", "0x00"]
    },
    {
      "type": "delay",
      "milliseconds": 50
    },
    {
      "type": "key_up",
      "report": ["0x00", "0x00", "0x00", "0x00", "0x00", "0x00", "0x00", "0x00"]
    }
  ]
}