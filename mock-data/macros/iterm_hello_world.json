{
  "id": "iterm_hello_world",
  "name": "iTerm Hello World",
  "description": "Opens iTerm via Spotlight and types 'hello world!'",
  "commands": [
    {
      "type": "key_press",
      "report": ["0x08", "0x00", "0x2C", "0x00", "0x00", "0x00", "0x00", "0x00"]
    },
    {
      "type": "delay",
      "milliseconds": 300
    },
    {
      "type": "type_text",
      "text": "iTerm"
    },
    {
      "type": "delay",
      "milliseconds": 500
    },
    {
      "type": "key_press",
      "report": ["0x00", "0x00", "0x28", "0x00", "0x00", "0x00", "0x00", "0x00"]
    },
    {
      "type": "delay",
      "milliseconds": 1000
    },
    {
      "type": "type_text",
      "text": "echo \"hello world\""
    },
    {
      "type": "delay",
      "milliseconds": 300
    },
    {
      "type": "key_press",
      "report": ["0x00", "0x00", "0x28", "0x00", "0x00", "0x00", "0x00", "0x00"]
    }
  ]
} 